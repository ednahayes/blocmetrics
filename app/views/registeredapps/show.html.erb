<div class="container">
  <h1><%= @registeredapp.name %></h1>
</div>
<div class="container">
	<h4>URL: <%= link_to @registeredapp.url, "#{@registeredapp.url}" %></h4>
</div>	
<div class="container">
    <small>
     submitted <%= time_ago_in_words(@registeredapp.created_at) %> ago
     by <%= @registeredapp.user.email %>
    </small> 
</div> 
<div class="container">
    <h4>There are <%= @events.count %> number of events for this application</h4>
        <% @events.each do |event, count| %>
         <ul>
            <li><%= "#{event}" %></li>
         </ul>
        <% end %>
</div>
<div class="container">
    <%= link_to "Edit Registered Application", edit_registeredapp_path, class: 'btn btn-success' %>
    <%= link_to "Delete Registered Application", @registeredapp, method: :delete, class: 'btn btn-danger', data: { confirm: 'Are you sure you want to delete this application?' } %>
</div>
